<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aac">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".java.MainActivity"
            android:theme="@style/MainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".java.data.activity.TestDataActivity" />
        <activity android:name=".java.data.activity.TestDataListActivity" />
        <activity android:name=".java.data.fragment.list.TestDataFragmentActivity" />
        <activity android:name=".java.data.fragment.data.Test2FragmentActivity" />
        <activity android:name=".java.data.fragment.TestFragmentActivity" />
    </application>

</manifest>
